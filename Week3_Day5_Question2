import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int N = sc.nextInt();
        if (N < 0) {
            System.out.println("Invalid Input");
        } else {
            for (int i = 0; i <= N; i++) {
                System.out.println(i);
            }
            System.out.println("Games End");
        }
        sc.close();
    }
}
